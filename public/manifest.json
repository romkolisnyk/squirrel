{
  "short_name": "Squirrel",
  "name": "Squirrel - New Tab Page",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "128": "squirrel_128.png",
    "48": "squirrel_48.png",
    "16": "squirrel_16.png"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "topSites",
    "management",
    "tabs",
    "fontSettings",
    "sessions"
  ],
  "options_page": "index.html?options",
  "options_ui": {
    "page": "index.html?options",
    "chrome_style": false,
    "open_in_tab": true
  },
  "web_accessible_resources": [
    "index.html"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
